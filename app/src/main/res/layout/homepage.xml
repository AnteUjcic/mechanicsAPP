<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomePage">

    <Button
        android:id="@+id/btnUpisServisa"
        android:layout_width="196dp"
        android:layout_height="56dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="34dp"
        android:text="Upis servisa"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnUpisNovogVozila"
        android:layout_width="198dp"
        android:layout_height="55dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="32dp"
        android:text="Upis novog vozila"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnUpisServisa" />

    <Button
        android:id="@+id/btnPregledVozila"
        android:layout_width="193dp"
        android:layout_height="57dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="37dp"
        android:text="Pregled vozila"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnUpisNovogVozila" />

    <Button
        android:id="@+id/btnPregledServisa"
        android:layout_width="192dp"
        android:layout_height="57dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="42dp"
        android:text="Pregled Servisa"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnPregledVozila" />

    <Button
        android:id="@+id/btnLogoutUser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="248dp"
        android:layout_marginTop="445dp"
        android:text="Logout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>